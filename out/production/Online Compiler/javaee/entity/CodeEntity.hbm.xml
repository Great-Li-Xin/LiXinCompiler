<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="javaee.entity.CodeEntity" table="code" schema="javaee">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="uploadtime">
            <column name="uploadtime" sql-type="datetime"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(10000)" length="10000" not-null="true"/>
        </property>
        <many-to-one name="username" column="username"/>
    </class>
</hibernate-mapping>